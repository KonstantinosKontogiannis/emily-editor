{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "settings": {
        "import/core-modules": [
            "styled-jsx/css"
        ]
    },
    "rules": {
        "no-console": 0,
        "react/jsx-filename-extension": 0
    }
}