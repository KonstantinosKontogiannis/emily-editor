{
  "name": "emily-editor",
  "version": "0.7.5",
  "description": "React editor component for LMLs, like Markdown or Asciidoc. The focus of the project is to provide fluent efficient interface for advanced users, who are familliar with using IDE or coding text editors.",
  "keywords": [
    "editor",
    "react",
    "markdown",
    "asciidoc",
    "markup",
    "lml",
    "ide",
    "text",
    "ace"
  ],
  "main": "index.js",
  "author": "Jaroslav Šmolík <grissius@gmail.com>",
  "license": "ISC",
  "scripts": {
    "dev": "node demo.js",
    "prestart": "next build",
    "start": "NODE_ENV=production node demo.js",
    "build": "next build",
    "prepare": "babel src --out-dir dist",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "./node_modules/jest/bin/jest.js",
    "test:watch": "./node_modules/jest/bin/jest.js --watch",
    "test:coverage": "./node_modules/jest/bin/jest.js --coverage",
    "test:ceveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/enzym.config.js",
    "setupFiles": [
      "./test/shim.js"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*"
    ]
  },
  "dependencies": {
    "ace-builds": "^1.2.9",
    "asciidoctor-html5s": "0.1.0-beta.5",
    "asciidoctor.js": "^1.5.6-preview.4",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.1",
    "glob": "^7.1.2",
    "highlight.js": "^9.12.0",
    "import": "0.0.6",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "markdown-it": "^8.4.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "md5": "^2.2.1",
    "next": "^5.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-sortable-hoc": "^0.6.8",
    "screenfull": "^3.3.1",
    "styled-jsx": "^2.2.0",
    "twemoji": "^2.5.0",
    "typo-js": "^1.0.3",
    "webpack": "^4.0.1",
    "babel-cli": "^6.26.0"    
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.12.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^22.4.2",
    "react-test-renderer": "^16.0.0"
  }
}
